{
  "name": "single-vue-page",
  "version": "1.0.1",
  "description": "single-vue-page, webpack project template for vue",
  "main": "./src/index.js",
  "scripts": {
    "startDev": "webpack-dev-server --progress --config build/webpack.dev.js --mode development --env=dev",
    "startTest": "webpack-dev-server --progress --config build/webpack.dev.js --mode development --env=test",
    "mock": "webpack-dev-server --progress --config build/webpack.dev.js --mode development --env=mock",
    "lint": "eslint --ext .js,.vue src",
    "build": "webpack --progress --progress --bail --config build/webpack.prod.js --mode production",
    "buildMock": "webpack --progress --progress --bail --config build/webpack.prod.js --mode production --env=mock",
    "buildTest": "webpack --progress --progress --bail --config build/webpack.prod.js --mode production --env=test",
    "buildStage": "webpack --progress --progress --bail --config build/webpack.prod.js --mode production --env=stage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Heanes/single-vue-page.git"
  },
  "keywords": [
    "vue",
    "template"
  ],
  "author": "Heanes",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Heanes/single-vue-page/issues"
  },
  "homepage": "https://github.com/Heanes/single-vue-page#readme",
  "devDependencies": {
    "autoprefixer": "^9.8.4",
    "babel-eslint": "^10.1.0",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "css-loader": "^3.6.0",
    "eslint": "^7.4.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-friendly-formatter": "^4.0.1",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-html": "^6.0.2",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "eslint-plugin-vue": "^6.2.2",
    "file-loader": "^6.0.0",
    "friendly-errors-webpack-plugin": "^1.7.0",
    "html-webpack-plugin": "^4.3.0",
    "mini-css-extract-plugin": "^0.9.0",
    "mockjs": "^1.1.0",
    "node-notifier": "^7.0.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "portfinder": "^1.0.26",
    "postcss-loader": "^3.0.0",
    "sass": "^1.26.9",
    "sass-loader": "^9.0.1",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.6",
    "url-loader": "^4.1.0",
    "vue-loader": "^15.9.3",
    "vue-style-loader": "^4.1.2",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.43.0",
    "webpack-bundle-analyzer": "^3.8.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0",
    "webpack-module-hot-accept": "^1.0.5"
  },
  "dependencies": {
    "axios": "^0.19.2",
    "vue": "^2.6.11",
    "vue-router": "^3.3.4",
    "vuex": "^3.5.1"
  }
}
